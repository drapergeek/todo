<!DOCTYPE html>
<html>
<head>
  <title>Todo</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% if current_user %>
		Welcome, <%= current_user %> <%= current_user.last_name %>
		<%= link_to "Logout", sessions_path, method: :delete %>
	<% end %>
	<p><%= flash[:notice] %></p>
	<%= yield %>

</body>
</html>
